<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="Dominos" namespace="Dominos.Models">
  <class name="Product" table="Products" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="ProductID" column="ProductID" type="int">
      <generator class="native" />
    </id>

    <property name="ProductName"  />
    <property name="ProductImage"  />
    <property name="ProductPrice"  />
    
  </class>
</hibernate-mapping>