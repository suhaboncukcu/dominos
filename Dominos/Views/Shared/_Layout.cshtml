<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ASP.NET Uygulamam</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/custom.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-fixed-top dominos-navbar">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img class="navbar-brand" src="https://www.dominos.com.tr/_assets/img/logo.png?v=15781" />
            </div>
            <div class="collapse navbar-collapse pull-right">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="list-unstyled list-inline">
                            <li><a href="">Girişi Yap</a></li>
                            <li><a href="">Üye Ol</a></li>
                        </ul>
                    </div>
                    <div class="col-md-12 text-right">
                        Sepet
                    </div>
                </div>
            </div><!--/.nav-collapse -->
        </div>
    </nav>

        @RenderBody()

    <div class="dominos-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-3">
                    Menü
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="https://unpkg.com/vue"></script>
    @Scripts.Render("~/Scripts/custom.js")
</body>
</html>
